<class name="Main">
	<subroutine type="function" name="main">
		<parameters />
		<locals>
			<variable>
				<class name="SquareGame" />
				<name>
					game
				</name>
			</variable>
		</locals>
		<body>
			<let>
				<access>
					<var name="game" />
				</access>
				<expression>
					<call>
						<qualified namespace="SquareGame" name="new" />
					</call>
				</expression>
			</let>
			<do>
				<qualified namespace="game" name="run" />
			</do>
			<do>
				<qualified namespace="game" name="dispose" />
			</do>
			<return />
		</body>
	</subroutine>
</class>
